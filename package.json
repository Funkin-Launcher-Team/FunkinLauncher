{
  "name": "funkin-launcher",
  "version": "1.6.5",
  "description": "An AIO engine manager for Friday Night Funkin'",
  "main": "src/Bootstrap.js",
  "isAlpha": false,
  "scripts": {
    "test": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build-linux": "electron-builder --linux AppImage tar.gz",
    "build-osx": "electron-builder --mac zip",
    "build-win": "electron-builder --win zip nsis"
  },
  "build": {
    "productName": "Funkin Launcher",
    "artifactName": "funkin-launcher.${ext}",
    "asar": false,
    "appId": "net.toperri.FunkinLauncher",
    "win": {
      "target": "zip nsis"
    },
    "mac": {
      "category": "public.app-category.games",
      "target": "zip"
    },
    "linux": {
      "category": "Game",
      "target": "AppImage tar.gz"
    }
  },
  "author": "toperri",
  "license": "MIT",
  "devDependencies": {
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3",
    "express": "^4.19.2",
    "fs-move": "^6.0.0",
    "progress": "^2.0.3",
    "request": "^2.88.2",
    "zip-lib": "^1.0.4"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "express": "^4.19.2",
    "file-type": "^19.4.0",
    "fs-move": "^6.0.0",
    "json-beautify": "^1.1.1",
    "lite-url": "^1.0.5",
    "node-7z": "^3.0.0",
    "node-unrar-js": "^2.0.2",
    "plist": "^3.1.0",
    "progress": "^2.0.3",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "unrar": "^0.2.0",
    "zip-lib": "^1.0.4"
  }
}
